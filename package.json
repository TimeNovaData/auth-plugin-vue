{
  "name": "novadata-vue-auth",
  "version": "2.1.2",
  "description": "Authentication plugin for Vue 3 with JWT refresh token support",
  "author": "NovaData",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
