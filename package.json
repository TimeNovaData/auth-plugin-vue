    "vue-router",
    "typescript"
  ],
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "axios": "^1.0.0",
    "@types/node": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimeNovaData/auth-plugin-vue"
  }
